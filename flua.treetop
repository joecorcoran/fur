grammar Flua
  rule program
    list+ <Program>
  end

  rule list
    space? '(' members:(list / string / integer / symbol / space)* ')' space? <List>
  end

  rule string
    '"' body:[^"]* '"' <String>
  end

  rule integer
    [\d]+ <Integer>
  end

  rule symbol
    [A-Za-z0-9_-]+ <Symbol>
  end

  rule space
    [\s\t\n\r]+ <Space>
  end
end
